; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Too many frogs!"
run/main_scene="res://menu/Control.tscn"
config/icon="res://assets/player/Owlet_Monster/Owlet_Monster.png"

[autoload]

Global="*res://Global.gd"

[display]

window/size/width=854
window/size/height=480
window/size/resizable=false
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

android=true
export=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

2d/snapping/use_gpu_pixel_snap=true
environment/default_clear_color=Color( 0.0901961, 1, 0.219608, 1 )
environment/default_environment="res://default_env.tres"
